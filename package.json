    "figlet": "^1.5.0",
    "got": "^11.8.2",
    "json2csv": "^5.0.6",
    "pino": "^6.13.1",
    "pino-pretty": "^6.0.0",
    "spinnies": "^0.5.1",
    "stream": "^0.0.2"
